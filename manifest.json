{
  "manifest_version": 3,
  "name": "Amazon商品検索キーボードショートカット",
  "author": "evesquare",
  "version": "1.0",
  "description": "⌘+K(win+K)でAmazon商品検索テキストボックスにフォーカスします。",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.amazon.co.jp/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": []
}